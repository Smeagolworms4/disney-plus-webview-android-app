{
  "manifest_version": 2,
  "id": "customExtension@disney.com",
  "name": "customExtension",
  "version": "1.0",
  "type": "extension",
  "description": "Arrow navigation.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "nativeMessaging",
    "nativeMessagingFromContent",
    "geckoViewAddons"
  ]
}
